<form method = "POST" action = "/songs">
<p><label>Name:</label><input type = "text" id = "Name" name = "song_name"></p>
<p><label>Artist Name:</label><input type = "text" id = "Artist Name" name = "artist_name"></p>
<p><label>Genres: </label></p>
<% Genre.all.each do |genre| %>
<input type = "checkbox" id = "<%= genre.name %>" name = "song[genres][]" value = "<%= genre.id %>"><%= genre.name %><br>
<% end %>
<br>
<input type = "submit" id = "Create" value = "Create">